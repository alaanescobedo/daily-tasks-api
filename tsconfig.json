{
  "extends": "@tsconfig/node16-strictest-esm/tsconfig.json",
  "compilerOptions": {
  "module": "CommonJS",
  "baseUrl": ".",
  "paths": {
    "@task/*": [
      "src/task/*"
    ],
    "@config/*": [
      "src/config/*"
    ],
    "@lib/redis/*": [
      "src/lib/redis/*"
    ],
  }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}